# Sample environment variables for mockup-tools
# Copy this file to .env and fill in your actual API keys

# Etsy API credentials
# Required for Etsy integration in etsy/auth.py and etsy/listings.py
# Get these from https://www.etsy.com/developers/register
ETSY_API_KEY="your_etsy_api_key_here"
ETSY_API_SECRET="your_etsy_api_secret_here"

# Etsy Shop ID
# Required for Etsy integration in etsy/listings.py
# This is your Etsy shop ID number (optional, defaults to 42865309)
ETSY_SHOP_ID="your_etsy_shop_id_here"

GEMINI_API_KEY="your_gemini_api_key_here"
GEMINI_MODEL="gemini-2.0-flash"

export OPENAI_API_KEY="your_openai_api_key_here"
export OPENAI_MODEL="gpt-4.5-preview"

# Default AI provider (gemini or openai)
export AI_PROVIDER="openai"
